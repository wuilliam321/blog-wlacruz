{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Starter kit for React+Redux apps",
  "repository": "",
  "author": {
    "name": "Julian Reyes Escrigas",
    "email": "jreyes@bixlabs.com",
    "url": "http://twitter.com/rokemaster"
  },
  "license": "MIT",
  "private": true,
  "cmd": {
    "start": "nodemon --watch webpack.config.js ./node_modules/.bin/webpack-dev-server --hot --inline"
  },
  "betterScripts": {
    "start": "$npm_package_cmd_start",
    "build": {
      "command": "webpack -p --progress --colors --config webpack-prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "clean": "rm -rf dist/client/*",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "prestart": "npm run lint",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "start": "better-npm-run start",
    "build": "webpack -p --progress --colors",
    "build:prod": "npm run clean && better-npm-run build"
  },
  "devDependencies": {
    "autoprefixer": "^8.4.1",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-autobind-class-methods": "^5.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-bixlabs": "^1.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.4",
    "postcss-loader": "^2.1.5",
    "react-hot-loader": "^4.1.2",
    "redux-freeze": "^0.1.5",
    "redux-logger": "^3.0.6",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "history": "^4.7.2",
    "hoek": "^5.0.3",
    "husky": "^0.14.3",
    "json-loader": "^0.5.7",
    "lint-staged": "^7.1.0",
    "lodash": "^4.17.5",
    "prettier": "^1.12.1",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0"
  }
}
